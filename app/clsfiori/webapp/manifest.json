{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.app.clsfiori",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.7",
      "toolsId": "341c974d-9688-42dc-8405-4fbcc3685268"
    },
    "dataSources": {
      "mainService": {
        "uri": "/LibrarySystemSRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      },
      "mainServicev2": {
        "uri": "/odata/v2/LibrarySystemSRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "CLSFioriApp-display": {
          "semanticObject": "CLSFioriApp",
          "action": "display",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.131.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.app.clsfiori.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "ModelV2": {
        "dataSource": "mainServicev2",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.app.clsfiori.view"
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": ":?query:",
          "target": [
            "TargetHome"
          ]
        },
        {
          "name": "RouteBooks",
          "pattern": "Books",
          "target": [
           "TargetBooks"
          ]
        },
        {
          "name": "RouteSingleUserPage",
          "pattern": "SingleUserPage",
          "target": [
           "TargetSingleUserPage"
          ]
        },
        {
          "name": "RouteUsers",
          "pattern": "Users",
          "target": [
           "TargetUsers"
          ]
        }

      ],
      "targets": {
        "TargetHome": {
          "id": "Home",
          "name": "Home"
        },
        "TargetBooks": {
          "id": "Books",
          "name": "Books"
        },
        "TargetSingleUserPage": {
          "id": "SingleUserPage",
          "name": "SingleUserPage"
        },
        "TargetUsers": {
          "id": "Users",
          "name": "Users"
        }
      }
    },
    "rootView": {
      "viewName": "com.app.clsfiori.view.App",
      "type": "XML",
      "id": "App"
    }
  }
}
